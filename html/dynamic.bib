@Article{zeitz2014,
  Title                    = {Minimalphasigkeit -- keine relevante Eigenschaft f{\"u}r die Regelungstechnik!},
  Author                   = {M. Zeitz},
  Journal                  = AT,
  Year                     = {2014},
  Number                   = {1},
  Pages                    = {3-10},
  Volume                   = {62},

  Abstract                 = {Ursprünglich wurde die Minimalphasigkeit von H. W. Bode eingeführt, um den eineindeutigen Zusammenhang zwischen Amplitude und Phase des Frequenzgangs zu definieren. Beim Entwurf von digitalen Filtern wird eine andere Definition der Minimalphasigkeit verwendet und ein asymptotisch stabiles Filter als minimalphasig bezeichnet, wenn auch das inverse Filter asymptotisch stabil ist. Schließlich werden von C. I. Byrnes und A. Isidori Systeme mit einer asymptotisch stabilen Nulldynamik als minimalphasig bezeichnet. Wegen der nicht konsistenten Definitionen wird dafür plädiert, den Minimalphasigkeits-Begriff in der Regelungstechnik zu vermeiden und die Stabilität von Filtern und der Nulldynamik mit den wohl definierten Kriterien von Hurwitz oder Ljapunow zu beschreiben.},
  File                     = {zeitz2014.pdf:zeitz2014.pdf:PDF}
}

@Article{zeitz2012at,
